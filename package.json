    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "name": "cz_customize"
    },
    "cz-customizable": {
      "config": ".cz-config.ts"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write '**/*.{ts,tsx}' --ignore-unknown"
    ]
