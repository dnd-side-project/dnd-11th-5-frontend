  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "name": "cz_customize"
    },
    "cz-customizable": {
      "config": ".cz-config.ts"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write '**/*.{ts,tsx}' --ignore-unknown"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "release-it": {
    "$schema": "https://unpkg.com/release-it@17/schema/release-it.json",
    "git": {
      "tag": true,
      "push": true,
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "‚ú® feat",
              "section": "Features"
            },
            {
              "type": "üêõ fix",
              "section": "Bug Fixes"
            },
            {
              "type": "üìù docs",
              "section": "Documentation"
            },
            {
              "type": "üíÖ style",
              "section": "Styles"
            },
            {
              "type": "‚ôªÔ∏è refactor",
              "section": "Code Refactoring"
            },
            {
              "type": "üë∑ cicd",
              "section": "CI/CD"
            },
            {
              "type": "‚ö°Ô∏è perf",
              "section": "Performance Improvements"
            },
            {
              "type": "‚úÖ test",
              "section": "Tests"
            },
            {
              "type": "üîß chore",
              "section": "Chores"
            },
            {
              "type": "üî® settings",
              "section": "Settings"
            }
          ]
        }
      }
    }
  }
